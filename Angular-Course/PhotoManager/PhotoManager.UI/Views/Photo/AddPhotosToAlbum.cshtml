@model int[]
@{
    Layout = "~/Views/Shared/_Layout.cshtml";

    var albumId = String.Empty;
    if (ViewBag.AlbumId != null)
    {
        albumId = ViewBag.AlbumId.ToString();
    }
}


<div class="title">
    @if (Model != null && Model.Any())
    {
        <div id="UserPhotos" data-album-id="@albumId">
            @foreach (var id in Model)
            {
                <div id="@id" class="list photo-item"></div>
            }
        </div>
        <div class="row">
            <input id="AddPhotoSubmit" class="form-control btn btn-primary" type="button" value="ADD PHOTO" />
        </div>
    }
</div>
